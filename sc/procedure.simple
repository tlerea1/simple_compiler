PROGRAM X;

CONST int = 6;
CONST bool = TRUE;

PROCEDURE INOUT(x:INTEGER;y:BOOLEAN):BOOLEAN;
	VAR var: INTEGER;
	BEGIN
	READ var;
	WRITE var
	RETURN y
END INOUT;

PROCEDURE STATEMENTS();
	VAR int: INTEGER;
	BEGIN
	int := 2;
	IF int = 4 THEN
		WRITE 4
	ELSIF int = 1 THEN
		WRITE 1
	ELSIF int = 2 THEN
		WRITE 2
	ELSE
		WRITE 0
	END
END STATEMENTS;


PROCEDURE BLANK();
END BLANK;

BEGIN
WRITE INOUT(int,bool);
STATEMENTS();
BLANK()

END X.