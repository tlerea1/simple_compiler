PROGRAM X;

CONST len=5;
TYPE array=ARRAY len OF INTEGER;
VAR int : INTEGER;
PROCEDURE fib(n : INTEGER) : INTEGER;
    VAR ret:INTEGER;
    BEGIN

    IF n = 0 THEN
        ret := 0
    ELSIF n = 1 THEN
        ret := 1
    ELSE
        ret := fib(n-2) + fib (n-1)
    END

    RETURN ret
END fib;

PROCEDURE copy(a,b:array);
    VAR i : INTEGER;
    BEGIN

    WHILE i < len DO
        WRITE i;
        b[i] := a[i];
        i := i + 1
    END

END copy;

PROCEDURE set(a :array);
    VAR i : INTEGER;

    BEGIN

    WHILE i < len DO
        WRITE i;
        a[i] := i;
        i := i + 1
    END

END set;

PROCEDURE print(a,b:INTEGER);
	BEGIN

	WRITE a;
	WRITE b
END print;

PROCEDURE globalTest();
	BEGIN

	WRITE int
END globalTest;

VAR x : INTEGER;
